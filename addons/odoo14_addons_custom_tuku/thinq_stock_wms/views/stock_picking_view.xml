        <record model="ir.ui.view" id="thinq_view_stock_quant_tree_inherit">
            <field name="name">thinq.stock.quant.tree.inherit</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree"></field>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_id']" position="before">
                    <field name="product_default_code" readonly="1"/>
                </xpath>
            </field>
        </record>

        <record model="ir.ui.view" id="thinq_view_stock_quant_tree_editable_inherit">
            <field name="name">thinq.stock.quant.tree.editable.inherit</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"></field>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='product_id']" position="before">
                    <field name="product_default_code" readonly="1"/>
                </xpath>
            </field>
        </record>
